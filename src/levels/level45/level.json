{
    "title": "Угон сессии: Куки",
    "cleanup": [
        {
            "action": "delete_file",
            "args": {
                "path": "client.js"
            }
        },
        {
            "action": "delete_file",
            "args": {
                "path": "output.txt"
            }
        }
    ],
    "validation": [
        {
            "validator": "command_succeeds",
            "args": {
                "command": "node client.js > output.txt"
            }
        },
        {
            "validator": "file_content_matches",
            "args": {
                "path": "output.txt",
                "content": "Welcome, agent"
            }
        }
    ]
}
