{
    "title": "Парсинг сайтов с `axios`",
    "cleanup": [
        {
            "action": "delete_file",
            "args": {
                "path": "api_fetcher.js"
            }
        },
        {
            "action": "delete_file",
            "args": {
                "path": "output.txt"
            }
        }
    ],
    "validation": [
        {
            "validator": "command_succeeds",
            "args": {
                "command": "node api_fetcher.js > output.txt"
            }
        },
        {
            "validator": "file_content_matches",
            "args": {
                "path": "output.txt",
                "content": "A cat"
            }
        }
    ]
}
