{
    "title": "Перемещение и переименование",
    "setup": [
        {
            "action": "create_file",
            "args": {
                "path": "old_name.txt"
            }
        },
        {
            "action": "create_file",
            "args": {
                "path": "move_me.txt"
            }
        },
        {
            "action": "create_directory",
            "args": {
                "path": "target_dir"
            }
        }
    ],
    "cleanup": [
        {
            "action": "delete_file",
            "args": {
                "path": "new_name.txt"
            }
        },
        {
            "action": "delete_directory",
            "args": {
                "path": "target_dir"
            }
        }
    ],
    "validation": [
        {
            "validator": "file_system_entry_exists",
            "args": {
                "shouldExist": false,
                "path": "old_name.txt"
            }
        },
        {
            "validator": "file_system_entry_exists",
            "args": {
                "path": "new_name.txt"
            }
        },
        {
            "validator": "file_system_entry_exists",
            "args": {
                "path": "target_dir/move_me.txt"
            }
        }
    ]
}
