{
    "title": "Создание нескольких файлов",
    "setup": [
        {
            "action": "create_directory",
            "args": {
                "path": "trainee_stuff/dir1"
            }
        }
    ],
    "cleanup": [
        {
            "action": "delete_directory",
            "args": {
                "path": "trainee_stuff"
            }
        }
    ],
    "validation": [
        {
            "validator": "file_system_entry_exists",
            "args": {
                "path": "trainee_stuff/my_notes.txt"
            }
        },
        {
            "validator": "file_system_entry_exists",
            "args": {
                "path": "trainee_stuff/dir1/cat_fact.txt"
            }
        }
    ]
}
