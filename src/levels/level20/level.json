{
    "title": "Трассировка маршрута",
    "cleanup": [
        {
            "action": "delete_file",
            "args": {
                "path": "trace.txt"
            }
        }
    ],
    "validation": [
        {
            "validator": "file_system_entry_exists",
            "args": {
                "path": "trace.txt"
            }
        },
        {
            "validator": "file_content_matches",
            "args": {
                "path": "trace.txt",
                "content": "google.com"
            }
        }
    ]
}
