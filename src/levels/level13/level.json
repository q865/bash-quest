{
    "title": "Парсинг JSON с `jq`",
    "setup": [
        {
            "action": "create_file",
            "args": {
                "path": "agents.json",
                "content": "[{\"name\":\"Ivanov\",\"status\":\"active\",\"codeword\":\"Vodka\"},{\"name\":\"Petrov\",\"status\":\"MIA\",\"codeword\":\"Balalaika\"},{\"name\":\"Sidorov\",\"status\":\"active\",\"codeword\":\"Matryoshka\"}]"
            }
        }
    ],
    "cleanup": [
        {
            "action": "delete_file",
            "args": {
                "path": "agents.json"
            }
        },
        {
            "action": "delete_file",
            "args": {
                "path": "codeword.txt"
            }
        }
    ],
    "validation": [
        {
            "validator": "file_system_entry_exists",
            "args": {
                "path": "codeword.txt"
            }
        },
        {
            "validator": "file_content_matches",
            "args": {
                "path": "codeword.txt",
                "content": "Balalaika"
            }
        }
    ]
}
