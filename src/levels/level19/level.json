{
  "title": "Удалённый доступ: ssh",
  "hint": "ssh root@192.168.1.100",
  "validation": [
    {
      "validator": "file_matches_regex",
      "args": {
        "path": "trainee_stuff/ssh_command.txt",
        "regex": "^ssh\\s+root@192\\.168\\.1\\.100$"
      }
    }
  ],
  "cleanup": [
    {
      "action": "delete_file",
      "args": {
        "path": "trainee_stuff/ssh_command.txt"
      }
    }
  ]
}