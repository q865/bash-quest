{
    "title": "Взлом баз: SQL-инъекции",
    "cleanup": [
        {
            "action": "delete_file",
            "args": {
                "path": "injector.js"
            }
        },
        {
            "action": "delete_file",
            "args": {
                "path": "output.txt"
            }
        }
    ],
    "validation": [
        {
            "validator": "command_succeeds",
            "args": {
                "command": "node injector.js > output.txt"
            }
        },
        {
            "validator": "file_content_matches",
            "args": {
                "path": "output.txt",
                "content": "SELECT * FROM users WHERE id = 1 OR 1=1"
            }
        }
    ]
}
