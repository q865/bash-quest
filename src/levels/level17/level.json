{
    "title": "Качаем сайты: wget",
    "cleanup": [
        {
            "action": "delete_file",
            "args": {
                "path": "local_copy.html"
            }
        }
    ],
    "validation": [
        {
            "validator": "file_system_entry_exists",
            "args": {
                "path": "local_copy.html"
            }
        },
        {
            "validator": "file_content_matches",
            "args": {
                "path": "local_copy.html",
                "content": "Example Domain"
            }
        }
    ]
}
