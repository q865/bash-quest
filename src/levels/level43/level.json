{
    "title": "Puppeteer: Эмуляция",
    "cleanup": [
        {
            "action": "delete_file",
            "args": {
                "path": "mobile_screenshot.js"
            }
        },
        {
            "action": "delete_file",
            "args": {
                "path": "mobile.png"
            }
        }
    ],
    "validation": [
        {
            "validator": "command_succeeds",
            "args": {
                "command": "node mobile_screenshot.js"
            }
        },
        {
            "validator": "file_system_entry_exists",
            "args": {
                "path": "mobile.png"
            }
        }
    ]
}
